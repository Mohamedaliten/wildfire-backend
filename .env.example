# .env - Backend Environment Configuration

# Server Configuration
PORT=3001
NODE_ENV=development

# AWS Configuration - REPLACE WITH YOUR ACTUAL VALUES
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# DynamoDB Table Configuration - UPDATE WITH YOUR ACTUAL TABLE NAME
DYNAMODB_TABLE_NAME=wildfire-sensor-data

# Table Schema Configuration - UPDATE AFTER RUNNING TEST SCRIPT
TIMESTAMP_FIELD=timestamp
DEVICE_ID_FIELD=deviceId
DEFAULT_DEVICE_ID=device_001

# CORS Configuration - UPDATE WITH YOUR ACTUAL DOMAINS
NEXTJS_URL=http://localhost:3000
WEBSITE_URL=https://yourdomain.com
PRODUCTION_DOMAIN=https://yourdomain.com
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:19006,http://10.0.2.2:3001

# Features
ENABLE_WEBSOCKET=true
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100